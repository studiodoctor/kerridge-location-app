<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="LocationApp.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:LocationApp"
    Shell.FlyoutBehavior="Disabled">

    <ShellContent
        Title="Home"
        ContentTemplate="{DataTemplate local:View.MainPage}"
        Route="View.MainPage" />

        <ShellContent
        Title="Description"
        ContentTemplate="{DataTemplate local:View.DescriptionPage}"
        Route="View.DescriptionPage" />

</Shell>
